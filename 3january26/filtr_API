import java.util.*;
public class filtr_api {
    public static void main(String[]args){
        ArrayList<Integer>list=new ArrayList<>();
        list.add(50);
        list.add(70);
        list.add(60);
        list.add(80);
       List<Integer> res= list.stream().filter(n->n>60).toList();
       System.out.println(res);
    }
}
