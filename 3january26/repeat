#include <iostream>
#include <vector>

int repeatedNTimes(std::vector<int>& nums) {
    int n = nums.size();

    for (int i = 0; i < n; ++i) {

        if (i + 1 < n && nums[i] == nums[i+1]) {
            return nums[i];
        }
        
        if (i + 2 < n && nums[i] == nums[i+2]) {
            return nums[i];
        }
        
        if (i + 3 < n && nums[i] == nums[i+3]) {
             return nums[i];
        }
    }
    return -1; 
}

int main() {
    std::vector<int> nums1 = {1, 2, 3, 3};
    std::cout << " 1: " << repeatedNTimes(nums1) << std::endl; }